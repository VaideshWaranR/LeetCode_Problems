class Solution {
    public int[] twoSum(int[] nums, int target) {
        
    
int arrsize=nums.length;
int[] result = new int [2];
for(int i=0;i<arrsize;i++){
  for(int j=i+1;j<arrsize;j++){
      if((nums[i]+nums[j])==target){
          result [0]=i;
      result[1]=j;
      return result;}
  }
  }
 return new int[0];
  }  
}